{
  "cli": {
    "version": ">= 0.60.0",
    "appVersionSource": "local"
  },
  "build": {
    "driver-preview": {
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease",
        "credentialsSource": "local"
      },
      "distribution": "internal",
      "channel": "preview",
      "env": {
        "APP_VARIANT": "driver",
        "APP_NAME": "ApoBasi Driver"
      }
    },
    "busminder-preview": {
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease",
        "credentialsSource": "local"
      },
      "distribution": "internal",
      "channel": "preview",
      "env": {
        "APP_VARIANT": "busminder",
        "APP_NAME": "ApoBasi BusMinder"
      }
    },
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "APP_VARIANT": "driver"
      }
    },
    "preview": {
      "android": {
        "buildType": "apk"
      },
      "distribution": "internal",
      "channel": "preview",
      "env": {
        "APP_VARIANT": "driver"
      }
    },
    "production": {
      "channel": "production",
      "env": {
        "APP_VARIANT": "driver"
      }
    }
  },
  "submit": {
    "production": {}
  }
}